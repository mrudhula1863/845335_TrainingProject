@model IEnumerable<Promeet_Booking_System.Models.Room>
    @{
        ViewBag.Title = "ViewRooms";
    }

    <table class="table">
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.RoomName)
            </th>


            <th>
                @Html.DisplayNameFor(model => model.Location)
            </th>




            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.RoomName)
                </td>


                <td>
                    @Html.DisplayFor(modelItem => item.Location)
                </td>




                <td>
                    <span class="glyphicon glyphicon-edit"> @Html.ActionLink("Edit", "Edit", new { id = item.RoomId }) </span>|
                    <span class="glyphicon glyphicon-saved"> @Html.ActionLink("Details", "Details", new { id = item.RoomId })</span> |
                    <span class="glyphicon glyphicon-erase">  @Html.ActionLink("Delete", "Delete", new { id = item.RoomId })</span>
                </td>
            </tr>
        }

    </table>

    <div>
        @Html.ActionLink("Back to VendorHome", "Index")
    </div>


