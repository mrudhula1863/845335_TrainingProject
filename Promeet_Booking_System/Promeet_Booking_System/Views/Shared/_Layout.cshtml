<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8">
    <meta http-equiv="cache-control" content="max-age=0">
    <meta http-equiv="cache-control" content="no-cache">
    <meta http-equiv="expires" content="0">
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT">
    <meta http-equiv="pragma" content="no-cache">
    <meta name="viewport" content="width=device-width, initial-scale=1">


    <title>@ViewBag.Title - PROMEET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")



</head>
<body style="background-image:url('/Content/Images/main bg.jpg');background-repeat:no-repeat;background-size:cover">
    <nav class="navbar navbar-default navbar-fixed-top navigation-container">
        <div class="navigation-container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">

                <img class="logo" src="~/Content/Images/meetingroom logo.jpg" alt="Logo" width="100" height="100">
                <a class="navbar-brand" style="color:white;font-size:larger;padding-top:30px;font-family:'Felix Titling'">promeet</a>
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    </button>

            </div>

            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                      @Html.Partial("_LoginPartial")
                </ul>
            </div>
        </div>
    </nav>
   
    
        @RenderBody()
        
      <p style="padding-top:300px">   &copy; Copyright Promeet 2020. All rights reserved. |</p>
        
    

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>
